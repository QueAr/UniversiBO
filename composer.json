{
    "name": "UniversiBO/UniversiBO",
    "description": "UniversiBO Web Portal",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "symfony/assetic-bundle": ">=2.1.0,<2.2.0",
        "symfony/swiftmailer-bundle": ">=2.1.0,<2.2.0",
        "symfony/monolog-bundle": ">=2.1.0,<2.2.0",
        "sensio/distribution-bundle": ">=2.1.0,<2.2.0",
        "sensio/framework-extra-bundle": ">=2.1.0,<2.2.0",
        "sensio/generator-bundle": ">=2.1.0,<2.2.0",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": ">=1.0.0,<1.1.0",
        "doctrine/data-fixtures": "1.0.*@dev",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "doctrine/migrations": "dev-master@dev",
        "doctrine/doctrine-migrations-bundle": "dev-master@dev",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "jms/security-extra-bundle": ">=1.2.0,<1.3.0",
        "jms/di-extra-bundle": ">=1.1.0,<1.2.0",
        "kriswallsmith/assetic": "1.1.*@dev",
        "twig/extensions": "1.0.*@dev",
        "universibo/shibboleth-bundle": "@dev",
        "zendframework/zend-feed": ">=2.0.0,<2.1.0",
        "universibo/core-bundle": "dev-master",
        "universibo/login-action-bundle": "dev-master"
    },
    "replaces": {
        "smarty/smarty": "v3.1.11"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/UniversiBO/UniversiboCoreBundle"
        },
        {
            "type": "vcs",
            "url": "http://github.com/UniversiBO/UniversiboLoginActionBundle"
        }
    ],
    "require-dev": {
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Associazione Universibo",
            "email": "associazione.universibo@unibo.it"
        }
    ]
}
